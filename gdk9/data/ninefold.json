{
  "name": "Ninefold Grid",
  "description": "Default Gdk9 principle: A1Z26, digital-root numbers, curated symbol energies.",
  "letter_mode": "a1z26",
  "number_mode": "digital_root",
  "normalize_zero_to_nine": true,
  "weights": {"letter": 1, "digit": 1, "symbol": 1},
  "harmonics": true,
  "symbol_energy": {
    ".": 1,
    ",": 2,
    "!": 3,
    "?": 4,
    ":": 5,
    ";": 6,
    "-": 7,
    "_": 8,
    "*": 9,
    "#": 5,
    "+": 4,
    "=": 6,
    "@": 8,
    "$": 7,
    "%": 5,
    "&": 9,
    "/": 3,
    "\\": 3,
    "(": 2,
    ")": 2,
    "[": 2,
    "]": 2,
    "{": 2,
    "}": 2,
    "<": 1,
    ">": 1,
    "|": 1,
    "~": 9,
    "^": 8
  }
}
